{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "airbnb-base"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "import/no-extraneous-dependencies": 0,
      "no-console": "off",
      "no-underscore-dangle": 0,
      "no-restricted-globals": 0,
      "linebreak-style": 0,
      "no-shadow": "off",
      "no-unused-vars": "off",
      "func-names": "off",
      "no-use-before-define": "off",
      "max-classes-per-file": "off",
      "no-undef": "off",
      "array-callback-return": "off",
      "consistent-return": "off",
      "arrow-body-style": "off"
    }
  }
  